{
  "name": "development",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-iis:ar": "ng build --configuration=production-ar --output-path=dist/iis/ar --deploy-url /ar/",
    "build-iis:en": "ng build --configuration=production-en --output-path=dist/iis/en --deploy-url /en/",
    "build-iis:fr": "ng build --configuration=production-fr --output-path=dist/iis/fr --deploy-url /fr/",
    "build-iis": "npm run build-iis:en && npm run build-iis:ar && npm run build-iis:fr",
    "serve-ar": "ng serve --aot --base-href /ar/ --configuration=production-ar --host 0.0.0.0 --open",
    "serve-en": "ng serve --aot --base-href /en/ --configuration=production-en --host 0.0.0.0 --open",
    "serve-fr": "ng serve --aot --base-href /fr/ --configuration=production-fr --host 0.0.0.0 --open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^8.1.4",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "^7.2.10",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-brands-svg-icons": "^5.10.2",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@ngx-share/button": "^7.1.3",
    "@ngx-share/buttons": "^7.1.3",
    "@ngx-share/core": "^6.0.1",
    "@sweetalert2/ngx-sweetalert2": "^5.0.3",
    "chart.js": "^2.8.0",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "ngx-sharebuttons": "^4.1.4",
    "ngx-social-button": "^1.0.4",
    "ngx-swiper-wrapper": "^7.2.1",
    "primeicons": "^1.0.0",
    "primeng": "^7.0.0",
    "rxjs": "~6.3.3",
    "sweetalert2": "^8.10.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
